    "electron-builder": "22.10.5",
    "electron-devtools-installer": "3.1.1",
    "eslint": "7.24.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "node-sass": "4.14.1",
    "postcss": "^8.2.10",
    "sass-loader": "10.0.2",
    "webpack": "4.44.2",
    "webpack-node-externals": "2.5.2"
  },
  "dependencies": {
    "element-ui": "2.15.1",
    "nuxt": "2.15.4",
    "nuxt-i18n": "^6.26.0"
